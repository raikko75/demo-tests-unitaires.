package services;

import javax.persistence.EntityManager;

import model.Livre;

public class DaoPizza implements IDaoPizza {
	private EntityManager em ;
	public DaoPizza(EntityManager em)
	{
		this.em = em;
	}

	public Livre find(int id) {
		Livre l  = em.find(Livre.class, id);
		if(l !=null){
			return l;

		}else
			return null;

	}

}
