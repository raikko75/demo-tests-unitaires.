package model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity 

@Table(name="livre")
public class Livre {

	@Id
	private Integer id;
	private String titre;
	private String auteur;
	public Livre() {
	
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}
}
